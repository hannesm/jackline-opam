bug-reports: "https://github.com/janestreet/stdio/issues"
depends: [
  "ocaml" {>= "4.04.1"}
  "base" {>= "v0.11" & < "v0.12"}
  "jbuilder" {build & >= "1.0+beta18.1"}
]
build: ["jbuilder" "build" "-p" name "-j" jobs]
